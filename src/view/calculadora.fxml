<AnchorPane id="AnchorPane" prefHeight="453.0" prefWidth="338.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CalculadoraController">
